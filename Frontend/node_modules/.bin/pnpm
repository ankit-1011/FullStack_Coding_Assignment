#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/ankit/OneDrive/Desktop/FullStack_Coding_Assignment/Frontend/node_modules/.pnpm/pnpm@10.20.0/node_modules/pnpm/bin/node_modules:/mnt/c/Users/ankit/OneDrive/Desktop/FullStack_Coding_Assignment/Frontend/node_modules/.pnpm/pnpm@10.20.0/node_modules/pnpm/node_modules:/mnt/c/Users/ankit/OneDrive/Desktop/FullStack_Coding_Assignment/Frontend/node_modules/.pnpm/pnpm@10.20.0/node_modules:/mnt/c/Users/ankit/OneDrive/Desktop/FullStack_Coding_Assignment/Frontend/node_modules/.pnpm/node_modules:/mnt/c/Users/ankit/OneDrive/Desktop/FullStack_Coding_Assignment/Frontend/node_modules:/mnt/c/Users/ankit/OneDrive/Desktop/FullStack_Coding_Assignment/node_modules:/mnt/c/Users/ankit/OneDrive/Desktop/node_modules:/mnt/c/Users/ankit/OneDrive/node_modules:/mnt/c/Users/ankit/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/ankit/OneDrive/Desktop/FullStack_Coding_Assignment/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/ankit/OneDrive/Desktop/FullStack_Coding_Assignment/Frontend/node_modules/.pnpm/pnpm@10.20.0/node_modules/pnpm/bin/node_modules:/mnt/c/Users/ankit/OneDrive/Desktop/FullStack_Coding_Assignment/Frontend/node_modules/.pnpm/pnpm@10.20.0/node_modules/pnpm/node_modules:/mnt/c/Users/ankit/OneDrive/Desktop/FullStack_Coding_Assignment/Frontend/node_modules/.pnpm/pnpm@10.20.0/node_modules:/mnt/c/Users/ankit/OneDrive/Desktop/FullStack_Coding_Assignment/Frontend/node_modules/.pnpm/node_modules:/mnt/c/Users/ankit/OneDrive/Desktop/FullStack_Coding_Assignment/Frontend/node_modules:/mnt/c/Users/ankit/OneDrive/Desktop/FullStack_Coding_Assignment/node_modules:/mnt/c/Users/ankit/OneDrive/Desktop/node_modules:/mnt/c/Users/ankit/OneDrive/node_modules:/mnt/c/Users/ankit/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/ankit/OneDrive/Desktop/FullStack_Coding_Assignment/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/pnpm@10.20.0/node_modules/pnpm/bin/pnpm.cjs" "$@"
else
  exec node  "$basedir/../.pnpm/pnpm@10.20.0/node_modules/pnpm/bin/pnpm.cjs" "$@"
fi
